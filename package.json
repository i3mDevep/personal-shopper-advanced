{
  "name": "personal-shopper-advanced",
  "version": "1.0.0",
  "description": "Serverless aws-nodejs-typescript template",
  "main": "serverless.ts",
  "lint-staged": {
    "*.{ts,js,tsx,jsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "scripts": {
    "lint": "eslint --ext js,jsx,ts,tsx .",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json}\"",
    "start.local": "sls offline start --stage localhost",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "dependencies": {
    "@middy/core": "^2.5.3",
    "@middy/http-error-handler": "^3.0.3",
    "@middy/http-json-body-parser": "^2.5.3",
    "@middy/validator": "^3.0.3",
    "aws-sdk": "^2.1145.0"
  },
  "devDependencies": {
    "@serverless/typescript": "^2.23.0",
    "@types/aws-lambda": "^8.10.71",
    "@types/node": "^14.14.25",
    "@vtex/prettier-config": "^0.3.6",
    "esbuild": "^0.14.11",
    "eslint": "^7.32.0",
    "eslint-config-vtex": "^14.1.1",
    "husky": "^7.0.2",
    "json-schema-to-ts": "^1.5.0",
    "lint-staged": "^11.1.2",
    "prettier": "^2.4.0",
    "serverless": "^2.23.0",
    "serverless-dotenv-plugin": "^4.0.1",
    "serverless-dynamodb-local": "^0.2.40",
    "serverless-esbuild": "^1.23.3",
    "serverless-offline": "^8.8.0",
    "serverless-offline-aws-eventbridge": "^1.6.6",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3"
  },
  "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
  "license": "MIT"
}
